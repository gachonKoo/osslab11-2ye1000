import geo.utils as utils

# calculate the length of hypotenuse(c) when a=3 and b=4
a, b = 3, 4
c = utils.pythagoras(a, b)  # utils 모듈에서 pythagoras 함수를 호출
print('c =', c)

# calculate the area of circle with radius r = 10
r = 10
area = utils.circle_area(r)  # utils 모듈에서 circle_area 함수를 호출
print('area =', area)
